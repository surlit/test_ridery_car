<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="vehicles_logs_list">
      <field name="name">vehicles_logs list</field>
      <field name="model">vehicles_logs.vehicles_logs</field>
      <field name="arch" type="xml">
        <tree create='False' delete="False" edit='False' default_order="create_date desc">
          <field name="status"/>
          <field name="text"/>
          <field name="create_date"/>
        </tree>
      </field>
    </record>

  
    <!-- actions opening views on models -->

    <record model="ir.actions.act_window" id="vehicles_logs.action_window">
      <field name="name">Logs</field>
      <field name="res_model">vehicles_logs.vehicles_logs</field>
      <field name="view_mode">tree</field>
    </record>

    <!-- Top menu item -->

    <menuitem name="Logs Vehicles" id="vehicle.menu_1" parent='vehicle.menu_root' action="vehicles_logs.action_window" />
    
  </data>
</odoo>